<div class="container">
    <h1>Create a new ticket</h1>

    <%= form_for @ticket do |f|%>
        <%= f.label :name %><br>
        <%= f.text_field :name%>
        <br>
        <br>
        <%= f.label :description %><br>
        <%= f.text_field :description%>

        <br>
        <br>
        <%= f.submit %>
    <% end%>
</div>